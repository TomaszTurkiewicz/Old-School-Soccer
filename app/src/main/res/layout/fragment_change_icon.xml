        android:id="@+id/fragment_change_icon_over_arms_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="naramienniki"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/fragment_change_icon_right_arrow_over_arms"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:id="@+id/fragment_change_icon_left_arrow_left_sleeve"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/fragment_change_icon_left_sleeve_id"
